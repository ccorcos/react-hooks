{
  "version": "2.3.4",
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "lib": [
      "es6",
      "es2017",
      "dom",
      "esnext.asynciterable"
    ],
    "target": "es5",
    "skipLibCheck": false,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
